h1 {
  font-family: Impact;
}

body {
  background-color: #00663b;
  font-family: sans-serif;
}

ul {
  font-family: Impact;
  font-size: 25px;
}

blockquote {
  color: white;
  font-size: 18px;
  font-style: oblique;
}

cite {
  color: #c5c5ff;
  position: relative;
  margin: 0 auto;
}

div {
  background-color: #00663b;
  clear: both;
  border-radius: 15px;
}

img {
  border: 3px solid #c5c5ff;
  border-radius: 200px;
}

#header {
  z-index: 1;
  position: fixed;
  width: 97.5%;
  margin: 0 auto;
  margin-top: -20px;
  height: 60px;
  background-color: black;
  margin-bottom: 10px;
  font-family: Verdana;
  border: 1px solid #c5c5ff;
}

#aboutme {
  float:left;
  margin-left: 700px;
  padding-top: 12px;
  font-size: 16px;
  font-family: Verdana;
  color: #ffffff;
}

#footer {
  width: 130px;
  margin: 0 auto;
  padding-top: 15px;
  font-size: 16px;
  font-family: Verdana;
  color: #ffffff;
  background-color: black;
  border-radius: 15px;
  border: 1px solid #c5c5ff;
}

#left {
  width:50px;
  height: auto;
  float: left;
  background-color: #00663b;
}

#right {
  width: 50px;
  height: auto;
  float: right;
  background-color: #00663b;
}

p1 {
  font-family: courier;
}

li {
  font-family: courier;
}

